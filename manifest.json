{
  "manifest_version": 2,

  "name": "Ibrik",
  "description": "Ibrik alpha",
  "version": "0.1",

  "content_scripts": [
      {
        "matches": ["*://*/*"],
        "css": [],
        "js": ["lib/jquery-2.1.3.js", "page.js"]
      }
  ],

  "browser_action": {
    "default_icon": "ibrik.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
